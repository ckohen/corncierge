    "eslint-config-prettier": "^6.15.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.6",
    "prettier": "^2.1.2",
    "lint-staged": "^10.5.3"
  },
  "engines": {
    "node": ">=14.0.0"
