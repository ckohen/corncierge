    "axios": "^0.21.1",
    "db-migrate": "^0.11.11",
    "db-migrate-mysql": "^2.1.1",
    "discord-youtube-api": "^0.1.0",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "ffmpeg": "0.0.4",
    "ffmpeg-static": "^4.2.6",
    "humanize-duration": "^3.23.1",
    "limiter": "^1.1.5",
    "memory-cache": "^0.2.0",
    "moment": "^2.27.0",
    "mysql2": "^1.7.0",
    "obs-websocket-js": "^4.0.1",
