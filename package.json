    "docs": "docgen --source app --output docs/docs.json",
    "docs:test": "docgen --sourec app",
    "lint": "eslint app",
    "lint:config": "eslint config",
    "lint:fix": "eslint app --fix",
    "lint:config:fix": "eslint config --fix",
    "prettier": "prettier --write app/**/*.js",
    "prettier:config": "prettier --write config/**/*.js"
